{
  "linters": {
    "disable-all": true,
    "enable": [
      "goimports",
      "errcheck",
      "gosimple",
      "govet",
      "ineffassign",
      "typecheck",
      "unused",
      "unparam",
      "prealloc",
      "gci"
    ]
  },
  "linters-settings": {
    "unparam": {
      "check-exported": true
    }
  }
}